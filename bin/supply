#!/bin/bash

echo "Hello World!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
echo $IDX

ls

mkdir tmp
cd tmp
mkdir deps
cd deps
mkdir IDX
cd IDX

touch config.yml

mkdir profile.d
cd profile.d
echo "echo 'this is a test'" > hello.sh
chmod +x hello.sh
echo "hello.sh created"
cat hello.sh
echo "end of hello.sh"
pwd
