#!/bin/bash

BUILD_DIR=$1
CACHE_DIR=$2
DEPS_DIR=$3
DEPS_IDX=$4

echo "Hello World!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"


cd $BUILD_DIR
ls -R


mkdir .profile.d
cd .profile.d
echo "echo 'this is a test'" > hello.sh
chmod +x hello.sh
echo "hello.sh created"
cat hello.sh
echo "end of hello.sh"
pwd
